      <name>space_type_object</name>
      <display_name>Apply the Measure to a Specific Space Type</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>schedule_object</name>
      <display_name>Replace the lighting schedule in the space type with the following schedule</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>cost_increase</name>
      <display_name>Increase in Material and Installation Costs ($).</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
